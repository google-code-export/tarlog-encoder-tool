<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="tarlog.encoder.category.run"
            name="Run Encoder">
      </category>
      <command
            categoryId="tarlog.encoder.category.run"
            id="tarlog.encoder.commands.run"
            name="Sample Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="tarlog.encodertool.eclipse.handlers.Handler"
            commandId="tarlog.encoder.commands.run">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="tarlog.encoder.toolbar">
            <command
                  commandId="tarlog.encoder.commands.run"
                  icon="icons/encoders.jpg"
                  id="tarlog.encoder.toolbar.run"
                  tooltip="Run Encoder">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="tarlog.encodertool.eclipse.preferences.EncoderToolPreferencePage"
            id="tarlog.encodertool.eclipse.preferences.EncoderToolPreferencePage"
            name="Encoder Tool Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="tarlog.encodertool.eclipse.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   </plugin>
